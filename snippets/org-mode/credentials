# -*- mode: snippet -*-
# name: credentials
# key: cred
# expand-env: ((level (1+ (org-outline-level-visible))))
# --
`(make-string level ?*)` $1
`(make-string level ? )` :PROPERTIES:
`(make-string level ? )` :USER: $2
`(make-string level ? )` :PASSWORD: `(generate-password)`$0
`(make-string level ? )` :LAST_CHANGED: `(format-time-string "[%Y-%m-%d %a %H:%M]")`
`(make-string level ? )` :END:
